#include<stdio.h>

int main()
{
/*A program to store and manage student details*/

char registrationNumber[20];
char firstName[30];
char lastName[25];
float intro_To_Programming;
float networking;
float accounting;
float operatingSystem;
float computerApplications;


    printf("enter your registrationNumber\n");
    scanf("%s",&registrationNumber);
    
    printf("firstName\n");
    scanf("%s",&firstName);
    
    printf("lastName\n");
    scanf("%s",&lastName);
    
    printf("intro_To_Programming\n");
    scanf("%f",&intro_To_Programming);
    
    printf("networking\n");
    scanf("%f",&networking);
    
    printf("operatingSystem\n");
    scanf("%f",&operatingSystem);
    
    printf("computerApplications\n");
    scanf("%f",&computerApplications);
    
    return 0;
}